{
  "name": "headbreaker",
  "version": "4.0.0",
  "description": "Jigsaw puzzles library for js",
  "main": "dist/headbreaker.js",
  "module": "dist/headbreaker.mjs",
  "types": "dist/headbreaker.d.ts",
  "exports": {
    ".": {
      "types": "./dist/headbreaker.d.ts",
      "import": "./dist/headbreaker.mjs",
      "require": "./dist/headbreaker.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "all": "npm run typecheck && npm test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flbulgarelli/headbreaker.git"
  },
  "author": "Franco Leonardo Bulgarelli",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/flbulgarelli/headbreaker/issues"
  },
  "homepage": "https://github.com/flbulgarelli/headbreaker#readme",
  "devDependencies": {
    "tsup": "^8.4.0",
    "typescript": "^5.7.3",
    "vitest": "^3.0.5"
  },
  "optionalDependencies": {
    "konva": "^9.3.18"
  }
}
